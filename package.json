{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix-routes && remix build",
    "dev": "concurrently \"remix-routes -w\" \"remix dev\"",
    "format": "prettier --config .prettierrc.json --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "tsn": "ts-node -r tsconfig-paths/register --require dotenv/config -O '{\"module\": \"commonjs\"}'",
    "db:migrate": "yarn tsn app/db/migration.ts",
    "db:seed": "yarn tsn app/db/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.5",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/vercel": "^1.7.2",
    "@remix-validated-form/with-zod": "^2.0.2",
    "@vercel/node": "^2.4.4",
    "csv-parse": "^5.3.1",
    "framer-motion": "^7.5.1",
    "jotai": "^1.8.4",
    "kysely": "^0.22.0",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-virtuoso": "^2.19.1",
    "remix-auth": "^3.3.0",
    "remix-auth-auth0": "^1.4.0",
    "remix-routes": "^0.5.0",
    "remix-validated-form": "^4.5.7",
    "use-debounce": "^8.0.4",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.1",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.4",
    "jest-extended": "^3.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "type-fest": "^3.0.0",
    "typescript": "^4.7.4",
    "vite-tsconfig-paths": "^3.5.1",
    "vitest": "^0.23.4"
  },
  "engines": {
    "node": ">=14"
  }
}
